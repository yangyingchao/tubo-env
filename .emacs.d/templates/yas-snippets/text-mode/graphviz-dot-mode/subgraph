# -*- mode: snippet -*-
# --
subgraph cluster_${1:Name} {
    node [shape=record fontsize=12 fontname=Courier style=filled];
    color = lightgray;
    style=filled;
    label = "${2:Label}";
    edge[color="#2e3436"  style=solid];

$1_entry[label = "${3:name}" shape=ellipse color="#4e9a06" style=filled];
$1_exit[label="Return" shape="ellipse" color="#ce5c00"];
}
